{
  "version": 2,
  "name": "nuam-tax-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next@latest"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_ENV": "production"
  },
  "functions": {
    "src/app/api/**/*.{js,ts}": {
      "runtime": "nodejs18.x"
    }
  },
  "outputDirectory": ".next"
}